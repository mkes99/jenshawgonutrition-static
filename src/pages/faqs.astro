import PageHero from "../components/PageHero.astro";
import CtaBarBooking from "../components/CtaBarBooking.astro";
import ProcessSection from "../components/ProcessSection.astro";
---

<BaseLayout>
  <title slot="head">FAQs | Jen Shawgo Nutrition</title>
  <PageHero
    title="Frequently Asked Questions"
    subtitle="“Finding the right answers begins with the right questions.” Frank Sonnenberg"
    image="/wp-content/uploads/2024/06/FAQ-header-scaled.jpg"
  />
  <main class="main__content">
    <section class="col-md-12 col-sm-12 col-xs-12 allow-gutenberg">
      <div class="row">
        <div class="container">
          <div class="wpb-content-wrapper">
              <div
                class="vc_row wpb_row vc_row-fluid faqs-mobile-padding content-dark data-bg-color-6961884f5b47e vc_custom_1717865264472 vc_row-has-fill vc_row-o-equal-height vc_row-o-content-middle vc_row-flex row-z-index-6961884f5b47e"
                data-vc-full-width="true"
                data-vc-full-width-init="false"
              >
                <div
                  class="wpb_column vc_column_container content-dark vc_col-sm-2 column-z-index-6961884f5b585"
                >
                  <div class="vc_column-inner vc_custom_1606274026269">
                    <div class="wpb_wrapper"></div>
                  </div>
                </div><div
                  class="wpb_column vc_column_container content-dark vc_col-sm-8 column-z-index-6961884f5b637"
                >
                  <div class="vc_column-inner vc_custom_1606285280894">
                    <div class="wpb_wrapper">
                      <p
                        class="vc_custom_heading sub-heading single pink font-800"
                        id="slcr_custom_heading_6961884f5ba38"
                      >
                        General Questions
                      </p><ul
                        class="accordion m_accordion6961884f5bd7d accordion__03 one-tab"
                      >
                        <li>
                          <div
                            class="accordion-header mh_accordion6961884f5bc92"
                          >
                            <h5 class="gap-0 mb-0">
                              What is integrative nutrition counseling?
                            </h5>
                          </div>
                          <div
                            class="accordion-content"
                            id="mhp_accordion6961884f5bc92"
                          >
                            <p>
                              Integrative nutrition counseling is a customized
                              approach to nutrition and lifestyle that aims to
                              promote optimal health and overall well-being for
                              each individual. It focuses on the root cause and
                              takes into account the whole person to help
                              prevent and combat chronic and metabolic
                              illnesses.
                            </p>
                          </div>
                        </li>
                        <li>
                          <div
                            class="accordion-header mh_accordion6961884f5bcd9"
                          >
                            <h5 class="gap-0 mb-0">How do I get started?</h5>
                          </div>
                          <div
                            class="accordion-content"
                            id="mhp_accordion6961884f5bcd9"
                          >
                            <p>
                              Start with a free 15-minute discovery call to talk
                              through your goals, coverage options, and next
                              steps. Be sure to check out the FAQs first to
                              learn more about what to expect. If everything
                              aligns, we'll move forward with scheduling your
                              initial consultation.
                            </p>
                          </div>
                        </li>
                        <li>
                          <div
                            class="accordion-header mh_accordion6961884f5bcf8"
                          >
                            <h5 class="gap-0 mb-0">
                              Can I meet with you in person for my sessions?
                            </h5>
                          </div>
                          <div
                            class="accordion-content"
                            id="mhp_accordion6961884f5bcf8"
                          >
                            <p>
                              At this time all sessions are conducted virtually
                              using a HIPAA-secure telehealth platform that
                              provides a convenient and safe way to connect, so
                              you can feel comfortable and confident during our
                              sessions.
                            </p>
                          </div>
                        </li>
                        <li>
                          <div
                            class="accordion-header mh_accordion6961884f5bd11"
                          >
                            <h5 class="gap-0 mb-0">
                              What ages do you work with?
                            </h5>
                          </div>
                          <div
                            class="accordion-content"
                            id="mhp_accordion6961884f5bd11"
                          >
                            <p>
                              Anyone 18 and over is invited to work with me.
                            </p>
                          </div>
                        </li>
                        <li>
                          <div
                            class="accordion-header mh_accordion6961884f5bd28"
                          >
                            <h5 class="gap-0 mb-0">
                              What are your credentials?
                            </h5>
                          </div>
                          <div
                            class="accordion-content"
                            id="mhp_accordion6961884f5bd28"
                          >
                            <p>
                              I hold a Master of Science in Clinical Nutrition
                              and am a board-certified Nutrition Specialist®
                              (CNS®), licensed dietitian nutritionist (LDN),
                              certified health and wellness coach, with
                              specialized training in nutritional genomics from
                              the American Nutrition Association (ANA).
                            </p>
                          </div>
                        </li>
                        <li>
                          <div
                            class="accordion-header mh_accordion6961884f5bd41"
                          >
                            <h5 class="gap-0 mb-0">
                              What is a Certified Nutrition Specialist®?
                            </h5>
                          </div>
                          <div
                            class="accordion-content"
                            id="mhp_accordion6961884f5bd41"
                          >
                            <p>
                              The Certified Nutrition Specialist® credential
                              (CNS®) is the most advanced certification for
                              personalized nutrition practitioners. Certified
                              Nutrition Specialists practice science-based
                              personalized nutrition therapy to power people to
                              health. This credential requires a Master of
                              Science degree in nutrition, completion of 1,000
                              hours of supervised practice experience, and
                              passing a rigorous board examination to
                              demonstrate their education, experience, and
                              training. They are also required to re-certify
                              every 5 years by completing at least 75 hours of
                              continuing education.
                            </p>
                          </div>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div><div
                  class="wpb_column vc_column_container content-dark vc_col-sm-2 column-z-index-6961884f5be1d"
